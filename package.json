      "uninstallDisplayName": "mella",
      "createDesktopShortcut": true
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "main.js",
      "dist1/**/*",
      "nsis/installer.nsh"
    ],
    "extends": null,
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
